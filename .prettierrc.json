{
  "tabWidth": 2,
  "printWidth": 120,
  "semi": true,
  "trailingComma": "es5",
  "singleQuote": true,
  "endOfLine": "lf",
  "overrides": [
    {
      "files": "*.hbs",
      "options": {
        "parser": "html"
      }
    },
    {
      "files": "*.css",
      "options": {
        "printWidth": 80
      }
    }
  ]
}
